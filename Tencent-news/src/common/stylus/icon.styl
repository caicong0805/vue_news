@font-face {
  font-family: 'news-icon';
  src:  url('../fonts/news-icon.eot?a4tbwi');
  src:  url('../fonts/news-icon.eot?a4tbwi#iefix') format('embedded-opentype'),
    url('../fonts/news-icon.ttf?a4tbwi') format('truetype'),
    url('../fonts/news-icon.woff?a4tbwi') format('woff'),
    url('../fonts/news-icon.svg?a4tbwi#news-icon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'news-icon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-newspaper:before {
  content: "\e904";
}
.icon-bookmarks:before {
  content: "\e9d3";
}
.icon-smile:before {
  content: "\e9e1";
}
.icon-play2:before {
  content: "\ea15";
}
